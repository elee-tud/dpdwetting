temperature     1.0     ;temperature

;integrator      emin    ;energy minimization integrator
;maxforce        100     ;maximum force to terminate energy minimization

integrator      vv      ;velocity verlet integration
timestep        0.06    ;integration time step
totalsteps      50000  ;total integration steps

nonbonded       dpd     ;nonbonded interaction type
bondlength      harmonic ;bondlength interaction type          

;randseed        123123  ;random seed
gamma           3.0     ;coupling parameter of the drag force
cellcutoff      1.0     ;cutoff distance of cells

rmcomvfreq      10      ;frequency of removing center-of-mass motion
;rmcomdir        x y     ;direction of the center-of-mass motion removal

;dumpbinary      no      ;output format as ASCII
xtrjfreq        100     ;frequency of writing positions and velocities
xlogfreq        10      ;frequency of writing log data
xstrfreq        500     ;frequency of writing stress
xfrcfreq        500     ;frequency of writing force

;pullspringk     0.01       ;spring constant to pull all particles
;pullcenter      10 10 10    ;position of the center of the pulling 

;wall            solid z    ;Reflective boundary condition
;wallposz        5.0 65.0   ;position of walls

slipspring      16000        ;number of slip springs
sscutoff        0.5 2.0     ;minimum and maximum distance cutoff of the new slip-springs
seqnmcsteps     50         ;number of MC steps per sequence
seqndpdsteps    50         ;number of DPD steps per sequence
ssparam         2.0 0.0     ;spring constant and equilibrium distance of slip springs
